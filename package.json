{
  "name": "q3",
  "version": "0.0.0",
  "description": "A Javascript library for solving quadratic equations set",
  "main": "q3.js",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "mocha": "~2.2.4",
    "chai": "*"
  },
  "scripts": {
    "test": "mocha",
    "tsc": "tsc",
    "build": "npm run build-js && npm run build-definition",
    "build-js": "npm run tsc -- --outFile q3.js q3.ts",
    "build-definition": "npm run tsc -- -d equations_solver.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qbolec/q3.git"
  },
  "keywords": [
    "TypScript",
    "3D",
    "Geometry",
    "Equations",
    "Quadratic"
  ],
  "author": "qbolec",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/qbolec/q3/issues"
  },
  "dependencies": {
    "lodash": "~4.17.11"
  },
  "devDependencies": {
    "typescript":"~3.3.3"
  }
}
